<body>
  <div class="container" style="padding-left: 0px;"> 
  <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a class="navbar-brand" href="#"><img src="/images/bus_front.jpg" width="30" height="30" alt=""></a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
      
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav mr-auto">
            <li class="nav-item active">
              <a class="nav-link" href="/home/<%=unm%>/<%=usermobile%>/">Home <span class="sr-only">(current)</span></a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/bookings/<%= usermobile%>">My Bookings</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/book/<%= usermobile%>">Find Bus</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/logout/">Logout</a>
              </li>
          </ul>
        </div>
      </nav>
      <br><br>
      <%if(error_messages){%>
      <h3 class="alerts alert-danger"><%=error_messages%></h3>  
      <%}%>
      <%if(success_messages){%>
        <h3 class="alerts alert-success"><%=success_messages%></h3> 
      <%}%>
    <div class="container">
    <div class="row">
        <h2 style="text-align: center; color: purple; font-family: fantasy;">YOUR BOOKINGS...</h2>
    </div>
    <div class="row">
    <%if (data.length==0){%>
        <h1>NO BOOKINGS...</h1>
    <%}else{%>
    <table class="table table-hover" style="overflow:auto;">
        <thead class="thead-dark">
        <tr>
            <th>NAME</th>
            <th>DETAILS</th>
            <th>SEAT</th>
            <th>CANCEL</th>
        </tr>
        </thead>
        <%for(let i=0;i<data.length;i++){%>
            <tr class="table-danger">
                <td style="color: rgb(31, 15, 241); font-family: lato; font-weight: bold;"><%=data[i].agent_id%><br><div style="color:black;">(<%=data[i].category%>)</div></td>
                <td>
                  <span style="font-weight: bold;color: green;"><%=data[i].origin%></span> to <span style="font-weight: bold;color: green;"><%=data[i].dest%></span></div>
                  <div style="font-weight: bold;color:mediumslateblue;"><%=(data[i].dot).toDateString()%>
                <br><div style="font-weight: bold;color: darkred;"><%=data[i].timing%></div></div></td>
                <td><%=data[i].seat_no%></td>
                <%if(label[i]==1){%>
                <td><button class="btn btn-primary" onclick="do_delete(<%=data[i].booking_id%>,'<%=data[i].user%>',<%=data[i].price%>)">CANCEL</button><br><br>
                <%}else{%>
                <td style="font-weight: 600; font-family: lato;">COMPLETED<br><br>
                <%}%>
                <%if(label[i]==1){%>
                  -</td>
                  <%}else{%>
                  <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#exampleModal" onclick="func1(<%=data[i].id%>,<%=usermobile%>)">
                    RATE
                  </button>
                  </td>
                  <%}%>
            </tr>
        <%}%>
    </table>    
    <%}%>
    </div>

    <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">RATE YOUR JOURNEY!</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body" id="modalbody">
        <span id="rateMe">
          <i class="fa fa-star py-2 px-1 rate-popover" data-index="0" data-html="true" data-toggle="popover"
            data-placement="top" title="Very bad"></i>
          <i class="fa fa-star py-2 px-1 rate-popover" data-index="1" data-html="true" data-toggle="popover"
            data-placement="top" title="Poor"></i>
          <i class="fa fa-star py-2 px-1 rate-popover" data-index="2" data-html="true" data-toggle="popover"
            data-placement="top" title="OK"></i>
          <i class="fa fa-star py-2 px-1 rate-popover" data-index="3" data-html="true" data-toggle="popover"
            data-placement="top" title="Good"></i>
          <i class="fa fa-star py-2 px-1 rate-popover" data-index="4" data-html="true" data-toggle="popover"
            data-placement="top" title="Excellent"></i>
        </span>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal" onclick="func2()">Close</button>
      </div>
    </div>
  </div>
</div>

    </div>
    </div>
